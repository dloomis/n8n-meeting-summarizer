    {
      "parameters": {
        "jsCode": "return items.map(item => {\n    const transcripts = item.json.data?.transcripts || [];\n    \n    // Parse extractedDate from dateCreated (Mac absolute time in seconds since 2001-01-01)\n    const dateCreated = item.json.data?.dateCreated || 0;\n    const unixTimestamp = dateCreated + 978307200; // Add offset to convert to Unix seconds\n    const extractedDate = new Date(unixTimestamp * 1000).toISOString().split('T')[0];\n    \n    // Default title (can override from filename if needed)\n    const title = 'Meeting Transcript';\n    \n    // Format transcripts with timestamps in MM:SS\n    const formattedTranscript = transcripts.map(seg => {\n        const speaker = seg.speaker?.name || 'Unknown';\n        const startSeconds = (seg.start || 0) / 1000; // ms to seconds\n        const minutes = Math.floor(startSeconds / 60);\n        const seconds = Math.floor(startSeconds % 60);\n        const timestamp = `${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;\n        return `${speaker}\\n${timestamp}\\n${seg.text}`;\n    }).join('\\n\\n');\n    \n    // Unique participants\n    const participants = [...new Set(transcripts.map(seg => seg.speaker?.name || 'Unknown').filter(name => name !== 'Unknown'))];\n    \n    return {\n        json: {\n            title,\n            extractedDate,\n            formattedTranscript,\n            participants\n        }\n    };\n});"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        832,
        1584
      ],
      "id": "9870f061-370b-4e18-969e-ad7587942c9a",
      "name": "Parse Transcript"
    },
    {
      "parameters": {
        "jsCode": "return items.map(item => {\n    const filename = $('Store Filename').first().json.transcriptionName || '';\n    \n    // Parses a filename for people after '!' , evergreen topic after '#', and focused topic after '+'.\n    // #, !, and + are optional and can be used separately.\n    function parseFilename(filename) {\n        let importantParticipants = [];\n        let evergreenTopic = null;\n        let focusedTopic = null;  // For +topic\n        \n        // Find the earliest special character to get base filename\n        const specialChars = ['!', '#', '+'];\n        let minIndex = filename.length;\n        for (const char of specialChars) {\n            const index = filename.indexOf(char);\n            if (index !== -1 && index < minIndex) {\n                minIndex = index;\n            }\n        }\n        const baseFilename = minIndex < filename.length ? filename.substring(0, minIndex) : filename;\n        \n        // Find '!' for importantParticipants\n        const bangIndex = filename.indexOf('!');\n        if (bangIndex !== -1) {\n            const namePart = filename.substring(bangIndex + 1); // Everything after !\n            importantParticipants = namePart.split(',')\n                .map(name => name.trim())\n                .filter(name => name.length > 0);\n        }\n        \n        // Find '#' for evergreenTopic\n        const hashIndex = filename.indexOf('#');\n        if (hashIndex !== -1) {\n            evergreenTopic = filename.substring(hashIndex + 1).trim(); // Everything after #\n        }\n        \n        // Find '+' for focusedTopic\n        const plusIndex = filename.indexOf('+');\n        if (plusIndex !== -1) {\n            focusedTopic = filename.substring(plusIndex + 1).trim(); // Everything after +, multi-word support\n        }\n        \n        return {\n            baseFilename,\n            importantParticipants,\n            evergreenTopic,\n            focusedTopic\n        };\n    }\n    \n    const { baseFilename, importantParticipants, evergreenTopic, focusedTopic } = parseFilename(filename);\n    \n    return {\n        json: {\n            baseFilename,\n            importantParticipants,\n            evergreenTopic,\n            focusedTopic\n        }\n    };\n});"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
